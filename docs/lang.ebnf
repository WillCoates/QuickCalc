digit = "1" | "2" | "3" | "4" | "5" | "6" | "7" | "8" | "9" | "0";

number = digit, { digit }, [ ".", digit, {digit} ], [ "e", digit, {digit} ];

additive = multiplicative
         | multiplicative, ('+' | '-'), additive;

multiplicative = expression
               | expression, ('*' | '/'), multiplicative;

expression = number
           | "(", additive, ")"
           | [ "+" | "-" ], expression;
